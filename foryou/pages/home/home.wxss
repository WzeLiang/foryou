/* pages/home/home.wxss */
#home{
  /* display: none; */
}
.swiper {
  height: 400rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.content{
  padding: 20rpx 20rpx 150rpx 20rpx;
  background: #F2F2F2;
}
.logobox{
  display: flex;
   justify-content: space-between;
   align-content:center;
   flex-wrap:  wrap ;
}
.logobox .logoitem{
  width: 225rpx;
  height: 100rpx;
  margin-top: 18rpx;
}
.logobox .logoitem image{
  width: 100%;
  height: 100%;
}
.section{
 margin-top:30rpx; 
}
.section .section-title{
  color: black;
  font-size: 30rpx;
  margin-bottom: 15rpx;
}
.tabbar{
  position: fixed;
  display: flex;
  justify-content: space-around;
  bottom: 0;
  left: 0;
  width: 100%;
  border-top:1rpx #eaeaea solid;
  background: white; 
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  padding: 20rpx 0;

}
.tabbar>view{
  width: 310rpx;
  
  border-radius: 8rpx;
}
.tabbardefault{
  border: 1rpx #eaeaea solid;
}
.tabbaractive{
  background: #2EA9DF;
  color: white;

}

.show{
  display: block;
}
.hidden{
  display: none;
}



/* member */
.member{
  display: none

}
.member-title{
  font-size: 40rpx;
  color: black;
  margin:40rpx 20rpx 20rpx;
}
.member-box{

}
.member-box .item{
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx #eaeaea solid;
  height: 100rpx;
  line-height: 105rpx;
  margin-left: 40rpx;
  padding-right: 40rpx;
  font-size: 30rpx;
}

.tab-active{
  display: block
}
